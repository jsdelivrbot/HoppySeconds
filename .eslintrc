{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": ["import", "jsx-a11y", "react"],
  "env": {
    "browser": true,
  },
  "rules": {
    "forbid-prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "new-cap": 0,
    "import/no-extraneous-dependencies": 0,
    "react/forbid-prop-types": 0,
  },
  "globals": {
    "API_URL": true
  }
}
